<div class="col-md-4">
  {{#each allMovies}}
    <div class="card">
      <img class="card-img-top" src="{{this.image}}" alt="{{this.title}} " />
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <a href="/movies/{{_id}}"> See details </a>
      </div>
    </div>
  {{/each}}
</div>